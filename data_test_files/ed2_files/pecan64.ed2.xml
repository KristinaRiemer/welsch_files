<?xml version="1.0"?>
<pecan>
  <outdir>/data/test_files/ed2_results</outdir>

  <database>
    <bety>
      <driver>PostgreSQL</driver>      
      <user>bety</user>
      <password>bety</password>
      <host>postgres</host>
      <dbname>bety</dbname>
      <write>TRUE</write>
    </bety>
    <dbfiles>/data/test_files/ed2_results/dbfiles</dbfiles>
  </database>

  <pfts>
    <pft>
      <name>ebifarm.c4grass.doe_vd</name> 
    </pft>
  </pfts>

  <ensemble>
    <variable>NPP</variable>
  </ensemble>

  <meta.analysis>
    <iter>3000</iter>
    <random.effects>FALSE</random.effects>
    <threshold>1.2</threshold>
    <update>AUTO</update>
  </meta.analysis>

  <sensitivity.analysis>
    <quantiles>
      <sigma>-1</sigma>
      <sigma>1</sigma>
    </quantiles>
    <variable>NPP</variable>
  </sensitivity.analysis>

  <model>
    <id>5000000001</id>
    <edin>/data/test_files/ed2_files/ED2IN.rgit</edin>
    <config.header>
      <radiation>
        <lai_min>0.01</lai_min>
      </radiation>
      <ed_misc>
        <output_month>12</output_month>
      </ed_misc>
    </config.header>
    <phenol.scheme>0</phenol.scheme>
  </model>

  <run>
    <site>
      <id>76</id>
      <met.start>2004-01-01 00:00:00</met.start>
      <met.end>2009-12-31 23:59:59</met.end>
    </site>
    <inputs>
      <met>/data/test_files/ed2_files/ed2_input_files/ebifarm/ED_MET_DRIVER_HEADER</met>
      <veg>/data/test_files/ed2_files/ed2_input_files/oge2OLD/OGE2_</veg>
      <soil>/data/test_files/ed2_files/ed2_input_files/faoOLD/FAO_</soil>
      <pss>/data/test_files/ed2_files/ed2_input_files/ebifarm/ebifarm.lat40.0lon-88.0.pss</pss>
      <css>/data/test_files/ed2_files/ed2_input_files/ebifarm/ebifarm.lat40.0lon-88.0.css</css>
      <site>/data/test_files/ed2_files/ed2_input_files/ebifarm/ebifarm.lat40.0lon-88.0.site</site>
      <lu>/data/test_files/ed2_files/ed2_input_files/ed_inputs/glu</lu>
      <thsum>/data/test_files/ed2_files/ed2_input_files/ed_inputs</thsum>
    </inputs>
    <start.date>2006/01/01</start.date>
    <end.date>2006/12/31</end.date>
  </run>

  <host>
    <rabbitmq>
      <uri>amqp://guest:guest@rabbitmq:5672/%2F</uri>
      <uri>amqp://guest:guest@rabbitmq/%2F</uri>
      <queue>ED2.2</queue>
    </rabbitmq>
  </host>

</pecan>
